{
  "id": "d1d75615-084d-4b9b-91a5-068b5f8c95a9",
  "name": "CondaClientInstaller",
  "friendlyName": "ms-resource:loc.friendlyName",
  "description": "ms-resource:loc.description",
  "helpUrl": "",
  "helpMarkDown": "ms-resource:loc.helpMarkDown",
  "category": "Package",
  "author": "Microsoft Corporation",
  "version": {
    "Major": 0,
    "Minor": 218,
    "Patch": 0
  },
  "runsOn": [
    "Agent",
    "DeploymentGroup"
  ],
  "minimumAgentVersion": "2.144.0",
  "instanceNameFormat": "ms-resource:loc.instanceNameFormat",
  "inputs": [
    {
      "name": "installLatest",
      "type": "boolean",
      "label": "ms-resource:loc.input.label.installLatest",
      "defaultValue": "true",
      "required": false,
      "helpMarkDown": "ms-resource:loc.input.help.installLatest"
    },
    {
      "name": "pythonMajorVersion",
      "type": "pickList",
      "label": "ms-resource:loc.input.label.pythonMajorVersion",
      "defaultValue": "3",
      "required": true,
      "helpMarkDown": "ms-resource:loc.input.help.pythonMajorVersion",
      "options": {
        "2": "Install for python 2",
        "3": "Install for python 3"
      }
    },
    {
      "name": "pythonMinorVersion",
      "type": "string",
      "label": "ms-resource:loc.input.label.pythonMinorVersion",
      "defaultValue": "",
      "required": false,
      "helpMarkDown": "ms-resource:loc.input.help.pythonMinorVersion"
    },
    {
      "name": "condaVersion",
      "type": "string",
      "label": "ms-resource:loc.input.label.condaVersion",
      "defaultValue": "",
      "required": false,
      "helpMarkDown": "ms-resource:loc.input.help.condaVersion"
    },
    {
      "name": "processorArchitecture",
      "type": "pickList",
      "label": "ms-resource:loc.input.label.processorArchitecture",
      "defaultValue": "x86_64",
      "required": true,
      "helpMarkDown": "ms-resource:loc.input.help.processorArchitecture",
      "options": {
        "x86_64": "",
        "x86": "",
        "arm64": "",
        "s390x": "",
        "ppc64le": "",
        "aarch64.sh": ""
      }
    },
    {
      "name": "installPath",
      "type": "text",
      "label": "ms-resource:loc.input.label.installPath",
      "defaultValue": "$(Agent.ToolsDirectory)/conda",
      "required": true,
      "helpMarkDown": "ms-resource:loc.input.help.installPath"
    }
  ],
  "execution": {
    "Node10": {
      "target": "condaclientinstallermain.js"
    }
  },
  "messages": {
    "UnsupportedOSPlatform": "ms-resource:loc.messages.UnsupportedOSPlatform",
    "FailedToRunTask": "ms-resource:loc.messages.FailedToRunTask",
    "FailedToRunCondaInstaller": "ms-resource:loc.messages.FailedToRunCondaInstaller",
    "FailedToInstallConda": "ms-resource:loc.messages.FailedToInstallConda"
  }
}